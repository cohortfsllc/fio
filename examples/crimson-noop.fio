# crimson-noop passes requests into a crimson::fio::Engine, but the Backend
# implementation returns immediately.  it's only useful as a comparison to
# other configurations

[global]
ioengine=crimson-noop
invalidate=0
gtod_reduce=1 # disable latency statistics that call gettimeofday a lot
rw=write
bs=1
size=512m

[noop]
iodepth=16
iodepth_batch_complete=16 # let getevents() spin with less interruptions
